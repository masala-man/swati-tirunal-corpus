[{"meta": {"name": "āj āyē", "type": "khayāl", "language": "hindī", "ragam": "yamunākalyāṇi", "talam": "aṭa"}, "sahityam": {"pallavi": [["āj āyē śyām mōhan rāsaṇḍal khēlnē"]], "anupallavi": [], "charanam": [["baṃsurī kī dhunak lāgē tān tanan natōṃ tanōṃ", "bājē taḷ mr̥daṃg kī dhun dhrukuṭ takiṭa tak dhrukuṭ tathoṃ"], ["nāc rahē gōrī tān thaiyā, thait, thai, tai, takiri tōṃ", "bājē pāyal, huṃchand, nanan, jhanan, nanan, tanan tōṃ"], ["tān gavēṃ takadīṃ tādi, dhirkat", "tak tōm tathōṃ dhiṃtakiṭ,", "taka dhinn, kiṭatak, dhiṃ takiṭ, tak, tak tathōm"], ["padmanābha tumhārī līlā kyākahūṃ mē sāṃvarō", "tāp saṃkaṭ śaraṇē yā sāhamārōtuṃharō"]]}}, {"meta": {"name": "āj unīndē", "type": "dhrupad", "language": "hindī", "ragam": "bibhās", "talam": "cau"}, "sahityam": {"pallavi": [["āj unīndē calē āyē ṭhāṭō mōrē aṅganā", "braj rāj nandakuvar māyi giridhari"]], "anupallavi": [], "charanam": [["kōmal kapōlan mē rāj jākē bindan", "añjan adhar lasē citra rūp dhārī"], ["mōsē kahō āvan aurat tō rahi pāyō", "mē tō jāgī rahī, ab bhōr bhayō pyārī"], ["padmanābha dīnabandhu chōḍē kaisē mōhē sakhī", "gōpināth sāṃvarō eri banvārī"]]}}, {"meta": {"name": "ānandavalli", "type": "kīrtanaṃ", "language": "saṃskr̥taṃ", "ragam": "nīlāmbarī", "talam": "ādi"}, "sahityam": {"pallavi": [["ānandavalli! kuru mudamavirataṃ", "ānandavalli!"]], "anupallavi": [["dīnajasantapatimirāmr̥tkiraṇāyitasuhasē", "dhr̥taśukapōtavilāsini jaya paraṃ"]], "charanam": [["jambhavimatamukhasēvitapadayugaḷē girirājasutē ghana-", "sāralasitavidhukhaṇḍasadr̥śaniṭilē", "śambhuvadanasarasīruhamadhupē", "sārasākṣi! hr̥di vihara divāniśaṃ"], ["kēśapāśajitasajalajaladanikarē padapaṅkajasēvaka-", "khēdajālaśamanaikaparamacaturē", "nāśitāghacaritē bhuvanatraya-", "nāyikē vitara mē śubhamanupamaṃ"], ["śāradēndurucimñjuḷatamavadanē munihr̥dayanivāsini", "cārukundmukuḷōpamavararadanē", "pārijātatarupallavacaraṇē", "padmnābhasahajē hara mē śucaṃ"]]}}, {"meta": {"name": "āndoḷikā vāhanē", "type": "utsavaprabhandaṃ", "language": "malayāḷaṃ", "ragam": "ānandabhairavi", "talam": "cāpu"}, "sahityam": {"pallavi": [["āndōḷikā vāhanē jagatpatiyā-", "mambhōjanābhan lasiccīṭunnu"]], "anupallavi": [], "charanam": [["indindiraṅṅaḷ koticciḻaluṃ madhu-", "tundilamāṃ sumamalikayaṇiññuṃ"], ["cāntēluṅkoṅkamār kaṇḍu mōhattāl", "centārsāyakanennuṃ manasi dhariccuṃ"], ["vēdāntārtthattin tattvamennuṃ sāra-", "vēdiyatāṃ muninivahamuṟccuṃ"], ["vāsavan mumpāṃ vānōraśēṣaṃ van-", "pēṟuṃ lōkaikanāthanennaṟiññuṃ"], ["bhaktajanamampināḻiyākuṃ śrī-", "padmanābhanennu dhīramuṟaccuṃ"]]}}, {"meta": {"name": "āñjanēya", "type": "kīrtanaṃ", "language": "saṃskr̥taṃ", "ragam": "sāvērī", "talam": "ādi"}, "sahityam": {"pallavi": [["āñjanēya raghurāmadūta mahitānubhāva pāhi"]], "anupallavi": [["kuñjaravarēṇyāyutasattva", "surañjitāmaramunīśvara santataṃ"]], "charanam": [["dinakarabhairavaśaiśavalīla", "dīptivinirjitakañcanaśaila", "ghanadaśakandharasōdaraśūla-", "khaṇḍanuśubhalīla", "janakasutādhivimōcanalōla", "jambumālikhalaniśicarakāla", "vanadhitaraṇacaṇanijalāṅgūla-", "vahnidagdhalañkānagara"], ["paramabhayānakasaṅgaradhīra", "prabalajavayutasamīrakumāra", "karatalaghātahatākṣakumāra", "kapivarakulaśūra", "suranarakinnaranutaguṇavāra", "sūryagr̥hītapadāgamasāra", "sarasakadaḷivipinavihāra", "caṇḍasiṃhikādaḷanānupāraṃ"], ["vimalamahauṣadhiśailānayana", "viśrutavirya supāṭalanayana", "damitavīrayūpākṣasaṃhanana", "daḷitāśōkavana", "vimatakampana kānanadahana", "vikaṭamuṣtimūrchitadaśavadana", "kamalanābhaśubhacaritaniśamana-", "kalitamōdabhara sādaramanudinaṃ"]]}}, {"meta": {"name": "ārādhayāmi", "type": "kīrtanaṃ", "language": "saṃskr̥taṃ", "ragam": "bilahari", "talam": "cāpu"}, "sahityam": {"pallavi": [["ārādhayāmi karaṇatrayēṇāhaṃ", "anudinamapi bhavantaṃ"]], "anupallavi": [["dhārādharābha budhādhinivāraṇa"]], "charanam": [["navamaṇimayapīṭhē nanu sannivēśayan", "aviḷambaṃ suśiśirēṇāmbhasā surabhiṇā", "savinayādaramahō saṃsnāpayāmīha", "bhuvanapāvanamūrttē bhuvi kimitō bhāgyaṃ!"], ["kamalārātidīdhitigaurimāvahēnāti-", "vimalavāsasā nirupama saṃvēṣṭayāmīśa", "amitagandhēnālaṃ aṅgarāgēṇa ca", "samalaṅkarōmi bhūṣaṇairavi varairahaṃ"], ["tuḷasīkundamallikādyurusūnajātēna", "kaḷadhautabhājanē kalpitēna pūjanaṃ", "naḷinakṣa racayāmi nātha dhūpadīpanamaṅ-", "gaḷanivēdyāni ca kalayē pañkajanābha"]]}}, {"meta": {"name": "āyē giridhar", "type": "khayāl", "language": "hindī", "ragam": "bhairavi", "talam": "ādi"}, "sahityam": {"pallavi": [["āyē giridhar mērē gōrī"]], "anupallavi": [], "charanam": [["añjan adhar lalāṭ mahābar nayan", "unīndē cal āyē rayan samay prabhu jhal bal", "karikē kōn tiyākubirmāyē"], ["bin guṇ māl virājat hīyamē br̥ḍhagal", "baiyyā sukh pāyē brajnārī ku bañcan karkē", "kaisē prītam sukh pāyē"], ["sōlā siṅgār kari phulan kē hār liyē", "vividh sugandh sē man māyē", "bairi thi mō man kē sāthi", "kumud sarōvar kumbhalāyē"], ["sukh kē kāraṇ duḥkh kē nivāraṇ", "madhuvan muraḷī dhun gāyē", "padmanābh prabhu phaṇi paraśāyi kōṭi mayan", "tan jhavi jhāyē"]]}}, {"meta": {"name": "abadh sukhadāyī", "type": "khayāl", "language": "hindī", "ragam": "kāpi", "talam": "ādi"}, "sahityam": {"pallavi": [["abadh sukhadāyī ab bājē badhāyī"]], "anupallavi": [], "charanam": [["ratan siṃhāsan kē par raghupati", "sītāsahit suhāyō", "rām bharat sumitrānandan ṭhāḍhē cāmar catur ḍulāyō"], ["gāv gāv jan maṅgal gāvat dēvan bajāyō", "rām rāvaṇ mārē, adur sab mārē, rāj vibhīṣaṇ pāyō"], ["māt kausalyā karat ārati nij man bāññcit pāyō", "rām padmanābh prabhu, phaniparaśayī tribhuvan sukh kari āyō"]]}}, {"meta": {"name": "ab tō bairāgin", "type": "ṭappā", "language": "hindī", "ragam": "khamās", "talam": "ādi"}, "sahityam": {"pallavi": [["ab tō bairāgin bhayi āli mētō", "ab bairāgin bhayi", "sēj siṅgār kachunahī bhāvē lukh sab bhul gayi"]], "anupallavi": [], "charanam": []}}, {"meta": {"name": "adrisutāvara", "type": "kīrtanaṃ", "language": "saṃskr̥taṃ", "ragam": "kalyāṇī", "talam": "ādi"}, "sahityam": {"pallavi": [["adrisutāvara! kalyāṇaśailaśarāsana!", "māmava śambhō!"]], "anupallavi": [["bhadrāpāradayāmbunighō! varapannagābharaṇa!", "lōkaśaraṇāniśaṃ"]], "charanam": [["dēvanadisamalaṇkr̥tanijajaṭa!", "dīnalōkaparipālaka! suvimala-", "bhāvayōgihr̥dayāmbujānilaya! su", "dhāvadātamr̥duhāsita! vimō!", "pāvanānupamacarita! nivārita-", "pāpajāla! vibudhaughavinatapada!", "sēvakākhilaviṣādaharaṇacaṇa!", "cittayōnidahanātimahita! para!"], ["śītabhānupr̥thukāvataṃsa! ripu-", "jātavāridharamāruta! nirupama!", "vitamōhamadakaitava! paṭutama-", "bhūtavr̥ndaparivr̥tasavidha!", "pūtanāmanikarāśritajanaghana-", "puṇyajāla! rajatādrikr̥tanilaya!", "bhūtidāyaka! manōjñasuguṇa! hari-", "sūtipāśupatasāyakada! satataṃ"], ["ghōrapuravipinadāva! mr̥kaṇḍuku-", "mārabhītivinivāraṇa! śaradija-", "sārasārasadaḷōpamalōcana", "sāmajājinadukūla vibhō!", "pūritāśritamanōratha śaṅkara", "bhāratīśasuranāyakasēvita!", "nāradādimunigītacarita! padma-", "nābhasēvakajanānukūla! paraṃ"]]}}, {"meta": {"name": "ahaha naiva janē", "type": "upākhyanaṃ", "language": "saṃskr̥taṃ", "ragam": "yamunākalyāṇi", "talam": "rūpaka"}, "sahityam": {"pallavi": [["ahaha naiva janē"]], "anupallavi": [], "charanam": [["kāñchanavaratnalasitagōpura-", "miha kiṃ madiyamahō dayārāma, rāmarāma?"], ["prapuragandha-divya dhūpa-surabhikusuma-", "parimiḷitaṃ lasatibhūridhāmdhāma dhāma"], ["śrīsarasījanābha! caraṇayugaḷavinata-", "lōkanikarakalitasarvakāma! kāmakāma"]]}}, {"meta": {"name": "ahō citta", "type": "kīrtanaṃ", "language": "saṃskr̥taṃ", "ragam": "śaṅkarābharaṇaṃ", "talam": "cāpu"}, "sahityam": {"pallavi": [["āhō citta! cintayā kintayā hanta tē viṣayēṣu"]], "anupallavi": [["bahuvyāmōhadāyiṣu duḥsvapnasamēṣu", "ēṣu dustyajamatarāgraharōṣakāriṣu"]], "charanam": [["mandahasitamadhumañju mukhāmbujaṃ", "indīvarākṣīṇāmiti mā mā gaṇaya", "syandipūtikaphasāndramidaṃ sarvaṃ", "nindinīyamiti nikhilabudhajananigaditaṃ khalu hā!"], ["kaḷakaḷabhaṣiṣu kathamapi calitēṣu", "kalitadarahasitakramamukhēṣu", "kalaya mā ratimamīṣu sutēṣu", "kaluṣanirayapātakaṇṭhabaddhaśilānukāriṣu hā!"], ["madabharamantharamātaṅgaturaṅgama-", "madanīyakr̥gamadamaṇiharmyādīni", "vada bada vastūni vapuṣi vilayajuṣi", "dadati mudaṃ kiyadacirarucilasitacapalāni hā!"]]}}, {"meta": {"name": "alam anagha", "type": "padaṃ", "language": "saṃskr̥taṃ", "ragam": "rītigauḷa", "talam": "jhampa"}, "sahityam": {"pallavi": [["alamanaghaviḷambēna hanta! tava vallabhā-", "malaghumadanasāyaka bhūnāmākalayāmburuhanayana"]], "anupallavi": [], "charanam": [["śiśirakarakiraṇātiśītaḷataḷimatalē", "viśithilatara nijadhairyā viluṭhati parama vivaśā", "viśati kadācana gēhaṃ virahakr̥ta kr̥śagātrā", "niśi niśi vicintyasā tvāṃ bhr̥śamahō vilapati bālā"], ["surabhi malaya samīramuragavaranana-", "garaḷamivakalayati gatamānasa paritōṣā", "hariṇamadaghusr̥ṇādhihārilēpanadikaṃ", "hariṇākṣi na dadhāti halahala vigatakutukā"], ["mama vacanamiha śr̥ṇu mānyaguṇanilaya", "ramaya tava jīvanāyikāṃ rajinīnāyakavadana", "samayōyamabhigantuṃ sā tava saṅgamēna", "kimapi hr̥di sukhamahaha kiratu śrīpadmanābha"]]}}, {"meta": {"name": "alarśaraparitāpaṃ", "type": "padaṃ", "language": "malayāḷaṃ", "ragam": "suruṭṭi", "talam": "cāpu"}, "sahityam": {"pallavi": [["alarśaraparitāpaṃ colvati-", "nnaḷivēṇi paṇi bālē"]], "anupallavi": [["jalajabandhuvumiha jaladhiyilaṇayunnu", "malyamārutamēṯṯu mama manamatitarāmbutavivaśamāyi", "-->sakhi"]], "charanam": [["vaḷarunnuhr̥di mōhamennōmalē", "taḷarunnu mama dēhaṃ kaḷamoḻi", "kusumavāṭikayatiluḷavāyōraḷikulārava-", "matihakēḷpatumadhikamādhinidānamayisakhi"], ["śaśiyuṃ ceṅkanalāyī sanprati suna-", "śaranum mē ripuvāyī", "śaśadharanērmukhisarasanōṭinimellē", "bhr̥śatarārttayatāmmamākhilaśucamayēkathayāśu", "-->sudarinī"], ["jaladharasadr̥śaśōbhanenkāntan śrī-", "jalajākṣanabjanābhān", "kalayati kimu kōpaṃ karuṇaveṭiññuḷḷilamalaṃ", "batatāmasēnakimihajavānmamasādhayēpsitaṃ"]]}}, {"meta": {"name": "āli mē tō jamunā", "type": "ṭappā", "language": "hindī", "ragam": "pūrvi", "talam": "ādi"}, "sahityam": {"pallavi": [["āli mē tō jamunā jal bharaṇan gayī", "jab śyām sundar su bhēṇt bhayī"]], "anupallavi": [], "charanam": [["mōran kē piñjh sīs bīrājat", "bāṃsurī mō upajat tān nayī"], ["gauvan kē saṅg kṣaṇ dhāvē kṣaṇ ṭhāḍhē", "gavāl bāl sē bōli bōlē amr̥tamayi"], ["sōyi padmanābh prabhu phaṇiraśāyi", "mōhē nihāl karē trilōkadayi"]]}}, {"meta": {"name": "Amunabhoomidevena", "type": "upākhyanaṃ", "language": "saṃskr̥taṃ", "ragam": "hamīrkalyāṇi", "talam": "ādi"}, "sahityam": {"pallavi": [["amunā bhūmidēvēnākāri kiṃ nanu", "puṇyamatidīna śarīrēṇa"]], "anupallavi": [], "charanam": [["mānitōyaṃ nanu sarōruhanābhēna", "bhāsurasuguṇārāmēṇa"]]}}, {"meta": {"name": "Anaamilo mahaboob", "type": "khayāl", "language": "hindī", "ragam": "bilahari", "talam": "ādi"}, "sahityam": {"pallavi": [["ān milō mehabub hamārō"]], "anupallavi": [], "charanam": [["hōvuṃ tōri dāsi lālā nanda kuvaṟ pyārō"], ["cun cun kaliyāṃ mē sēj banāvuṃ", "sēj palaṅg raṅg mahal tumārō"], ["atar abīr gulāb lagāvuṃ", "prēm kaṭōrī sē mōkuṃ nāth bisārō"], ["padmanābh prabhu phaṇiraśāyi", "kabahuṃ nahīṃ mōku nāth bisārō"]]}}, {"meta": {"name": "Bhogindrasaayinam", "type": "kīrtanaṃ", "language": "saṃskr̥taṃ", "ragam": "kuntalavarāḷi", "talam": "jhampa"}, "sahityam": {"pallavi": [["bhōgīndra śāyinaṃ purukuśaladāyinaṃ", "puruṣaṃ śāśvataṃ kalayē"]], "anupallavi": [["vāgīśa gaurīśa vāsavādyamarapari-", "vārādivanditapadaṃ padmanābhaṃ"]], "charanam": [["kṣiravārannidhiśvētāntarīpadavacitramaṇi-", "-->saudhāsthitaṃ", "nāradādyakhila muni divyapārṣadavihaganāyaka-", "-->sadārādhitaṃ", "surabhāsura sudarśana nandaka pramukha śōbhanāyudha", "-->sēvitaṃ", "śrīrasākarasarōjāmr̥ditapādavanditārujana", "-->bharaṇōdytaṃ padmanābhaṃ"], ["parama bhaktōttama divākarayanīndra kr̥ta guru puṇya", "-->vasudhāgataṃ", "surucirādbhuta bālavēṣa sākṣālkāra pari-", "tōṣa cira lāḷitaṃ", "paripujanārtha sāḷagrāma haraṇāttatara yōgikara", "-->vāritaṃ", "tvaritānuyāna yatikaruṇā kulānanta purapāda-", "-->pādyāsitaṃ padmanābhaṃ"], ["kalitanuta sanyāsi bhajanānuguṇa vihita-", "-->sulaḷitākāramahitaṃ", "kaluṣahara parisarōjjvala padmatīrtthādi", "-->khaṇḍitāśeṣa duritaṃ", "kalaśōdbhavānpita mahēndra trikutavaramalayā-", "calēndra vinutaṃ", "khaladamana vañci mārttāṇḍavarmmā kalita kāñcana-", "vimānalasitaṃ padmanābhaṃ"], ["aruṇābha maṇimakuṭa makarakuṇḍala tārahāra kaustubha-", "-->bhūṣitaṃ", "parivīta pītāmbarā kalita kañcikāvara vanyya-", "-->mālāñcitaṃ", "niravadya vañciśa kr̥talakṣadīpādi nityōtsavā-", "-->nanditaṃ", "sarasiruhōdara palāśāyatākṣi puṭa paripurṇṇa-", "-->karuṇāmr̥taṃ padmanābhaṃ"]]}}]